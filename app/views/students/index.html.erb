<h1>All Students</h1>
<ul id="students">
  <% @students.each do |student| %>
      <li id=student-<%= "#{student.id}" %>>
          <%= link_to student.name, student_path(student), class: 'student-name' %>
          <%= link_to "Delete", student_path(student), method: "delete" %>
      </li>
    <% end %>
</ul>
